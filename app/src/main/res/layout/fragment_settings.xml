<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#04141C">

    <!--<include-->
    <!--android:id="@+id/toolbar"-->
    <!--layout="@layout/toolbar_top"-->
    <!--android:layout_width="fill_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:visibility="visible" />-->

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="@dimen/_15sdp">

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tv_invite_fb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Invite Facebook Friends"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tv_find_contacts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Find Contacts"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tv_filter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Filters"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvStatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Status"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <!--<com.musicseque.Fonts.Noyhr-->
                <!--android:id="@+id/tvLanguage"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_marginTop="@dimen/_20sdp"-->
                <!--android:text="Language"-->
                <!--android:textColor="#FA7B43"-->
                <!--android:textSize="@dimen/_18sdp" />-->


            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvChangePassword"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Change Password"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvReportProblem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Report a Problem"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvTermsService"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Terms of Service"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp" />


            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvPrivacyPolicy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Privacy Policy"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp"/>

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvDeleteAccount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Delete Account"
                android:textColor="#FA7B43"
                android:textSize="@dimen/_18sdp"/>

            <com.musicseque.Fonts.Noyhr
                android:id="@+id/tvLogout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="LogOut"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/_18sdp"/>

        </LinearLayout>
    </ScrollView>


</RelativeLayout>
